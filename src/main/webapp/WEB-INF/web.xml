<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>ERS</display-name>
  <welcome-file-list>
    <welcome-file>/resources/login.html</welcome-file>
  </welcome-file-list>
  
 
  
    <servlet>
    <display-name>MasterServlet</display-name>
    <servlet-name>MasterServlet</servlet-name>
    <servlet-class>com.servlet.MasterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MasterServlet</servlet-name>
    <url-pattern>/api/user/*</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet>
    <description></description>
    <display-name>JsonMasterServlet</display-name>
    <servlet-name>JsonMasterServlet</servlet-name>
    <servlet-class>com.servlet.JsonMasterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>JsonMasterServlet</servlet-name>
    <url-pattern>/json/*</url-pattern>
  </servlet-mapping>
  
  
</web-app>